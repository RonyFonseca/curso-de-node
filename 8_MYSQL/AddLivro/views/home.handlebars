<div>
    <h1>Cadastre seu livro</h1>
    <form action="/books/insertbook" method="POST" class="form">
        <div class="form-control">
            <label for="title">Titulo</label>
            <input type="text" name="title" placeholder="Digite o titulo:">
        </div>
        <div class="form-control">
            <label for="page">Páginas</label>
            <input type="text" name="page" placeholder="Quantidade de páginas">
        </div>
        <div class="form-control">
            <label for="dc">Descrição doLivro</label>
            <input type="text" name="dc" placeholder="Descrição">
        </div>
        <input type="submit" value="cadastrar" id="cadastrar">
    </form>
</div>